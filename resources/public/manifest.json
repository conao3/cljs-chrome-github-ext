{
  "manifest_version": 3,
  "name": "Conao3 Chrome GitHub Extension",
  "version": "1.0.0",
  "description": "Chrome extension for GitHub powered by ClojureScript",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["js/content.js"]
    }
  ]
}
